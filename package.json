{
	"name": "sveltekit-skeleton-saas",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"predev": "npm run prisma:migrate:dev",
		"prisma:migrate:dev": "prisma migrate dev",
		"prisma:migrate:reset": "prisma migrate reset --force",
		"vercel-build": "prisma generate && prisma migrate deploy && npm run build"
	},
	"devDependencies": {
		"@playwright/test": "^1.33.0",
		"@skeletonlabs/skeleton": "^1.2.5",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/kit": "^1.16.2",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"prisma": "^4.13.0",
		"svelte": "^3.59.0",
		"svelte-check": "^3.3.1",
		"svelte-meta-tags": "^2.7.0",
		"sveltekit-superforms": "^0.8.6",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.5",
		"vitest": "^0.31.0",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@inlang/sdk-js": "^0.2.0",
		"@lucia-auth/adapter-prisma": "^2.0.0",
		"@prisma/client": "^4.13.0",
		"lucia-auth": "^1.5.0",
		"stripe": "^12.4.0"
	}
}
